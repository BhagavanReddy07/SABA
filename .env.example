# Copy this file to .env.local and fill in the values.

# Frontend -> Backend base URL
# Used by Next.js API routes to reach FastAPI locally and in production.
NEXT_PUBLIC_BACKEND_URL=http://127.0.0.1:8000

# Google Gemini (optional but recommended for AI responses)
GEMINI_API_KEY=

# App meta (optional)
NEXT_PUBLIC_APP_NAME=SABA
NEXT_PUBLIC_APP_DESCRIPTION=Your AI Personal Assistant
